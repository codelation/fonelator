<?xml version="1.0" encoding="UTF-8"?>
<Response>
  <Dial>
    <% if ENV["TWILIO_FORWARD_SIP"] %>
      <Sip username="<%= TWILIO_FORWARD_SIP_USERNAME %>" password="<%= TWILIO_FORWARD_SIP_PASSWORD %>">
        <%= ENV["TWILIO_FORWARD_SIP"] %>
      </Sip>
    <% else %>
      <Number><%= ENV["TWILIO_FORWARD_NUMBER"] %></Number>
    <% end %>
  </Dial>
</Response>